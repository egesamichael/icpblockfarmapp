{
  "dfx": "0.25.1",
  "version": 1,
  "canisters": {
    "icpblockfarmapp_backend": {
      "main": "src/icpblockfarmapp_backend/main.mo",
      "type": "motoko",
      "feature_flags": [
        "http_outcalls"
      ]
    },
    "icpblockfarmapp_frontend": {
      "type": "assets",
      "source": [
        "src/icpblockfarmapp_frontend/dist"
      ],
      "dependencies": [
        "icpblockfarmapp_backend"
      ],
      "workspace": "icpblockfarmapp_frontend"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "defaults": {
    "build": {
      "packtool": "mops sources",
      "args": ""
    }
  },
  "output_env_file": ".env"
}